package day09_ternary;

import java.util.Scanner;

public class C5_NestedTernary1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner scan = new Scanner (System. in);
		System.out.println("lutfen kontrol icin  uzunlugunu ve genisligini girin");
		
		double kenar1= scan.nextDouble();
		double kenar2= scan.nextDouble();
	
	
	// System.out.println(kenar1== kenar2 ?   "kare": "kare degil");
	
	String sonuc=kenar1>0&& kenar2>0? (kenar1== kenar2 ?   
			"kare": "kare degil") :"gecerli degil";
	
	System.out.println(sonuc);
	
	}

}
